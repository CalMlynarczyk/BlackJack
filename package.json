{
  "name": "blackjack",
  "version": "1.0.0",
  "description": "BlackJack portfolio project",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack -p",
    "start": "webpack-dev-server --open",
    "start:docker": "webpack-dev-server --host 0.0.0.0",
    "test": "mocha --require ts-node/register \"src/**/*.test.ts\"",
    "lint:ts": "eslint 'src/**/*.ts{x,}'",
    "lint:css": "stylelint --color src/**/*.css",
    "lint": "yarn lint:ts && yarn lint:css",
    "format": "yarn format:ts && yarn format:css && yarn format:misc",
    "format:ts": "eslint --fix src/**/*.ts{x,}",
    "format:css": "stylelint --color --fix src/**/*.css",
    "format:misc": "prettier --write src/**/*.html .circleci/config.yml *.json *.yml *.md"
  },
  "keywords": [
    "blackjack",
    "card",
    "game"
  ],
  "author": "Calvin Mlynarczyk",
  "license": "MIT",
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-motion": "0.5.2",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@types/chai": "4.2.11",
    "@types/mocha": "5.2.6",
    "@types/react": "16.9.42",
    "@types/react-dom": "16.9.8",
    "@types/react-motion": "0.0.27",
    "@types/react-redux": "7.1.9",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "8.1.0",
    "chai": "4.2.0",
    "core-js": "3.6.4",
    "css-loader": "2.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "file-loader": "3.0.1",
    "glob": "7.1.6",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.9.0",
    "mocha": "5.2.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.0.5",
    "style-loader": "0.23.1",
    "stylelint": "13.6.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-prettier": "1.1.2",
    "ts-node": "8.8.1",
    "typescript": "3.9.6",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
}
